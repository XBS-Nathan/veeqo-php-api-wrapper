{
    "name": "XBS-Internet/veeqo-php-api-wrapper",
    "description": "Veeqo PHP (Guzzle) API Wrapper",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "homepage": "http://www.thegeekhive.io",
    "authors": [
        {
            "name": "Nathan Jones",
            "email": "nathan@thegeekhive.io"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "~4.2"
    },
    "require": {
        "php": ">=5.5",
        "guzzlehttp/guzzle": "~5.3|~6.0",
        "guzzlehttp/guzzle-services": "0.5.0",
        "guzzlehttp/command": "0.7.0",
        "gimler/guzzle-description-loader": "^0.0.4"
    },
    "autoload": {
        "psr-4": {
            "Veeqo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Veeqo\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
